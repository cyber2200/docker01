CREATE TABLE event_log (
    id int NOT NULL AUTO_INCREMENT,
    event_type varchar(255) default '',
    event_msg text,
    PRIMARY KEY (id)
);

CREATE TABLE event_log(
   id SERIAL PRIMARY KEY,
   event_type VARCHAR NOT NULL,
   event_msg text
);

